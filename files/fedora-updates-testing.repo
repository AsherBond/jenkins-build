[updates-testing]
name=Fedora $releasever - $basearch - Test Updates
baseurl=http://chi-10g-1-mirror.fastsoft.net/pub/linux/fedora/linux/updates/testing/$releasever/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch

[updates-testing-debuginfo]
name=Fedora $releasever - $basearch - Test Updates Debug
baseurl=http://chi-10g-1-mirror.fastsoft.net/pub/linux/fedora/linux/updates/testing/$releasever/$basearch/debug/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch

[updates-testing-source]
name=Fedora $releasever - Test Updates Source
baseurl=http://chi-10g-1-mirror.fastsoft.net/pub/linux/fedora/linux/updates/testing/$releasever/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch
